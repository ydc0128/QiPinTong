<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:id="@+id/rl_master_foem_quyu"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="50px"
    android:layout_marginLeft="50px"
    android:layout_marginRight="50px"
    android:layout_marginBottom="50px">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="区域范围"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/colorBlack"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        >
        <TextView
            android:id="@+id/tv_master_foem_quyu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorBlack"/>
    <ImageView
        android:layout_width="40px"
        android:layout_height="40px"
        android:src="@mipmap/arrows"
        android:layout_gravity="center_vertical"
       />
    </LinearLayout>
</RelativeLayout>
    <include layout="@layout/finelinbackground"/>
    <RelativeLayout
        android:id="@+id/rl_master_foem_shuliang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:layout_marginBottom="50px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="数量要求"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorBlack"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        >
        <TextView
            android:id="@+id/tv_master_foem_shuliang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorBlack"/>
        <ImageView
            android:layout_width="40px"
            android:layout_height="40px"
            android:src="@mipmap/arrows"
            android:layout_gravity="center_vertical"
            />
    </LinearLayout>
    </RelativeLayout>
    <include layout="@layout/finelinbackground"/>
    <RelativeLayout
        android:id="@+id/rl_master_foem_danjia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:layout_marginBottom="50px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="共享单价"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorBlack"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            >
            <TextView
                android:id="@+id/tv_master_foem_danjia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/colorBlack"/>
            <ImageView
                android:layout_width="40px"
                android:layout_height="40px"
                android:src="@mipmap/arrows"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>
        </RelativeLayout>
    <include layout="@layout/finelinbackground"/>
    <RelativeLayout
        android:id="@+id/rl_master_foem_xingbei"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:layout_marginBottom="50px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="性别条件"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorBlack"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            >
            <TextView
                android:id="@+id/tv_master_foem_xingbei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/colorBlack"/>
            <ImageView
                android:layout_width="40px"
                android:layout_height="40px"
                android:src="@mipmap/arrows"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>
    </RelativeLayout>
    <include layout="@layout/finelinbackground"/>
    <RelativeLayout
        android:id="@+id/rl_master_foem_nianling"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:layout_marginBottom="50px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="年龄范围"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorBlack"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            >
            <TextView
                android:id="@+id/tv_master_foem_nianling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/colorBlack"/>
            <ImageView
                android:layout_width="40px"
                android:layout_height="40px"
                android:src="@mipmap/arrows"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>
    </RelativeLayout>
    <include layout="@layout/finelinbackground"/>
    <Button
        android:id="@+id/bt_master_form"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="确定"
        android:background="@drawable/shape_orange"
        android:textColor="@color/colorWhite"
        android:layout_marginTop="100px"
        android:layout_marginLeft="100px"
        android:layout_marginRight="100px"/>
</LinearLayout>