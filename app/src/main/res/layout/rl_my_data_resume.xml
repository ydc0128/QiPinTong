<?xml version="1.0" encoding="utf-8"?>
<cn.bingoogolapple.refreshlayout.BGARefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rf_my_data_resume"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
    <!--编辑简历——我的简历-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="140px"
        android:orientation="horizontal"

        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="140px"
            android:text="我的简历"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/colorWhite"
            android:gravity="center"
            android:background="@color/colorblue"

            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="30px"
            android:layout_centerVertical="true"
            >
            <ImageView
                android:layout_width="60px"
                android:layout_height="60px"
                android:background="@mipmap/back"
                android:layout_gravity="clip_vertical"
                />
        </LinearLayout>
    </RelativeLayout>
    <include layout="@layout/viewlinbackground"/>
    <include layout="@layout/viewlinbackground"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingRight="50px"
        android:paddingLeft="50px">
        <LinearLayout
            android:id="@+id/ll_my_data_resume_work"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="工作性质"
                    android:textColor="@color/colorBlack"
                   android:textSize="@dimen/sp_16"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:textColor="@color/colorred"/>

                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    android:id="@+id/editText" />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_post"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="期望职位"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:textColor="@color/colorred"/>
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_education_background"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="学历"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:textColor="@color/colorred"/>
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_age_limit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="工作年限"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:textColor="@color/colorred"/>
                <EditText
                    android:id="@+id/et_my_data_resume_mold_choice"
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>

        </LinearLayout>

        <include layout="@layout/finelinbackground"/>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_industry"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="期望行业"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>

        <LinearLayout
            android:id="@+id/ll_my_data_resume_area"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地区"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"/>
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>

        <LinearLayout
            android:id="@+id/ll_my_data_resume_identity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="求职身份"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"/>
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_status_of_job_seeking"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="求职状态"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:textColor="@color/colorred"/>
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"/>
            </LinearLayout>

        </LinearLayout>
        <include layout="@layout/finelinbackground"/>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_monthly_pay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
        <LinearLayout
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="月薪要求"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/sp_16"
                />
            <EditText
                android:layout_width="0px"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:hint=" "
                android:gravity="right"
                />
            <ImageView
                android:layout_width="40px"
                android:layout_height="50px"
                android:background="@mipmap/arrows"
                android:layout_gravity="center_vertical"/>
        </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_my_data_resume_my_evaluate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingTop="10px"
            android:paddingBottom="10px"
            >
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="自我评价"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/sp_16"
                    />
                <EditText
                    android:layout_width="0px"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:hint=" "
                    android:gravity="right"
                    />
                <ImageView
                    android:layout_width="40px"
                    android:layout_height="50px"
                    android:background="@mipmap/arrows"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>

        </LinearLayout>
        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="50px">
        <Button
            android:id="@+id/bt_my_data_resume_save"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorblue"
            android:text="保存"
            android:textColor="@color/colorWhite"
            android:textSize="@dimen/sp_16"
            />
    </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="60px"/>
</LinearLayout>
        </ScrollView>
    </cn.bingoogolapple.refreshlayout.BGARefreshLayout>
