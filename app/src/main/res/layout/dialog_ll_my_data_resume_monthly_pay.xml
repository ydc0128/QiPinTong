<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="30px"
        android:background="@color/colorlinegray">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="月薪"
            android:textColor="@color/black"
            android:textSize="16sp"/>
        <ImageView
            android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_back"
            android:layout_width="60px"
            android:layout_height="60px"
            android:background="@color/black"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="1-2k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_ll_my_data_resume_monthly_pay_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="2-3k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="3-5k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="5-8k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="8-10k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="10-12k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="12-15k"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
    <TextView
        android:id="@+id/tv_dialog_ll_my_data_resume_monthly_pay_8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="15k以上"
        android:padding="30px"
        android:textColor="@color/colorBlack"
        android:textSize="16sp"/>
    <include layout="@layout/finelinbackground"/>
</LinearLayout>